{
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/koa": "^2.11.3",
    "@types/koa-router": "^7.4.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.5",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^7.1.1",
