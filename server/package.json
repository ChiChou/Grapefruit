    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "name": "passionfruit-server",
  "version": "0.7.0",
  "description": "Runtime app instruments for iOS",
  "main": "dist/app.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "dependencies": {
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^8.0.8",
    "koa-send": "^5.0.0"
  },
  "scripts": {
    "test": "mocha",
    "build": "tsc",
    "dev": "nodemon"
  },
